{
  "manifest_version": 2,
  "homepage_url": "https://github.com/dodying/One-Click-Clean",
  "name": "__MSG_extName__",
  "version": "0.0.1",
  "default_locale": "en",
  "description": "__MSG_extDesc__",
  "icons": {
    "16": "images/logo-16.png",
    "48": "images/logo-48.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/logo-16.png",
      "48": "images/logo-48.png"
    }
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "options_ui": {
    "open_in_tab": true,
    "page": "options.html"
  },
  "permissions": [
    "<all_urls>",
    "browsingData",
    "contextMenus",
    "cookies",
    "notifications",
    "tabs"
  ]
}
